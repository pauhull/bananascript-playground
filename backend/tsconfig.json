    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2021",
        "strict": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "outDir": "dist/",
        "lib": ["ES2021"]
    }
}
